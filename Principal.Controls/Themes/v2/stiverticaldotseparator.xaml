<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:controls="clr-namespace:Principal.Controls.Core">
    <Style x:Key="{x:Type controls:StiVerticalDotSeparator}" TargetType="{x:Type controls:StiVerticalDotSeparator}">
        <Setter Property="UIElement.IsHitTestVisible" Value="False" />
        <Setter Property="Width" Value="1" />
        <Setter Property="Focusable" Value="False" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type controls:StiVerticalDotSeparator}">
                    <Rectangle Fill="{TemplateBinding controls:StiVerticalDotSeparator.FillSeparator}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>